  "license": "MPL-2.0",
  "keywords": ["APXC" ,"open", "connections" , "BugFix" ],
  "homepage": "https://github.com/APXc/node-red-contrib-apxc-getconnections#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/APXc/node-red-contrib-apxc-getconnections.git"
  },
  "bugs": {
    "url": "https://github.com/APXc/node-red-contrib-apxc-getconnections/issues",
    "email": "support@apserial.it"
  },
  "node-red": {
    "version": ">=2.0.0",
    "nodes": {
      "getconnections" : "/nodes/getConnections.js"
    }
  },
   "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node-red-node-test-helper": "^0.2.2",
    "node-red": "^3.0.2",
    "node-red-node-test-helper": "^0.3.0"
  }
}
