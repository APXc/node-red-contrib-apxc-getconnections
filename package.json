{
  "name": "@apxc/node-red-contrib-apxc-getconnections",
  "version": "0.0.1",
  "description": "get opened connections on NodeRed Installations",
  "main": "/nodes/getConnections.js",
  "scripts": {
    "update": "npm pack && mv node-red-contrib-apxc-getconnections-$npm_package_version.tgz ./data && cd data && npm i node-red-contrib-apxc-getconnections-$npm_package_version.tgz && docker-compose restart",
    "win" : "start winupd.bat"
  },
